{
  "name": "trew-starter",
  "version": "1.0.21",
  "description": "Production-ready Typescript-React-ExpressJS-Webpack website starter",
  "files": [
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .jsx --ext .js,.ts,.tsx src/ --fix",
    "test-unit": "jest --config ./src/test/unit/jest.config.ts",
    "test-e2e-dev": "concurrently \"npm start\" \"cypress run --config-file ./cypress.json --project ./src/test/e2e\"",
    "test-e2e-prod": "npm run build && concurrently \"npm run start-prod\" \"cypress run --config-file ./cypress.json --project ./src/test/e2e\"",
    "test": "npm run test-unit && npm run test-e2e-dev",
    "start": "env-cmd -e dev nodemon --ext \"ts,json\" --ignore ./src/client --exec \"npx ts-node --project ./tsconfig/dev.json ./src/server/app.ts\"",
    "clean": "rimraf ./build && rimraf ./webpack/base.js && rimraf ./webpack/prod.js",
    "build-server": "rimraf ./build/src && env-cmd -e prod tsc -p ./tsconfig/serverProd.json",
    "build-webpack-prod-config": "rimraf ./webpack/base.js && rimraf ./webpack/prod.js && env-cmd -e prod tsc -p ./tsconfig/webpackProd.json",
    "build-client": "rimraf ./build/client && env-cmd -e prod webpack --progress --config ./webpack/prod.js",
    "build": "npm run build-server && npm run build-webpack-prod-config && npm run build-client",
    "start-prod": "env-cmd -e prod node ./build/src/server/app.js",
    "docker-build": "docker build -t trew-starter:local ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samhuk/trew-starter"
  },
  "author": "samhuk",
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@types/clone-deep": "^4.0.1",
    "@types/compression-webpack-plugin": "^6.0.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "clone-deep": "^4.0.1",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-sass": "^5.0.0",
    "react-redux": "^7.2.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/react-redux": "^7.1.16",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-merge": "^4.1.5",
    "compression-webpack-plugin": "^7.1.2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.2",
    "cypress": "^6.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "express-static-gzip": "^2.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.6",
    "nodemon": "^2.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.3"
  }
}
