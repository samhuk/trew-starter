{
  "name": "trew-starter",
  "version": "1.0.21",
  "description": "Production-ready Typescript-React-ExpressJS-Webpack website starter",
  "files": [
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .jsx --ext .js,.ts,.tsx src/ --fix",
    "start": "env-cmd -e dev nodemon --ext \"ts,json\" --ignore ./src/client --exec \"ts-node --project ./tsconfig/dev.json ./src/server/app.ts\"",
    "clean": "rm -rf ./build && rm -f ./webpack/base.js && rm -f ./webpack/prod.js",
    "build-server": "rm -rf ./build/src && env-cmd -e prod tsc -p ./tsconfig/serverProd.json",
    "build-webpack-prod-config": "rm -f ./webpack/base.js && rm -f ./webpack/prod.js && env-cmd -e prod tsc -p ./tsconfig/webpackProd.json",
    "build-client": "rm -rf ./build/client && env-cmd -e prod webpack --progress --config ./webpack/prod.js",
    "build": "npm run build-server && npm run build-webpack-prod-config && npm run build-client",
    "start-prod": "env-cmd -e prod node ./build/src/server/app.js",
    "docker-build": "docker build -t trew-starter:local .",
    "docker-build-no-npm-install": "mv ./Dockerfile ./Dockerfile.backup && cp ./Dockerfile.no-npm-install ./Dockerfile && docker build -t trew-starter:local . || mv ./Dockerfile.backup ./Dockerfile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samhuk/trew-starter"
  },
  "author": "samhuk",
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/clone-deep": "^4.0.1",
    "@types/compression-webpack-plugin": "^6.0.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.2",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "clone-deep": "^4.0.1",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "react-redux": "^7.2.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.3",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/react-redux": "^7.1.16",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
